{
  "name": "functions",
  "version": "1.0.0",
  "main": "lib/index.js",
  "license": "MIT",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build": "tsc",
    "start": "functions-framework --source=lib/ --target=helloWorld",
    "dev": "concurrently \"tsc -w\" \"nodemon --watch ./lib/ --exec yarn start\"",
    "typecheck": "tsc --noEmit",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run lint"
  },
  "dependencies": {},
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.1.2",
    "@types/node": "^14.11.2",
    "concurrently": "^7.2.2",
    "gts": "^3.1.0",
    "nodemon": "^2.0.18",
    "typescript": "^4.7.4"
  }
}
